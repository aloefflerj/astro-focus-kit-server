{
  "name": "astro-focus-kit-server",
  "version": "1.0.0",
  "description": "Astro Focus Kit Server - Academic application that tries to solve procrastination",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/index.js",
    "dev": "ts-node-dev 'src/index.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "prettier:check": "prettier --config .prettierrc 'src/**/*.ts' 'test/**/*.ts' --check",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' 'test/**/*.ts' --write",
    "prettier:watch": "onchange 'src/**/*.ts' 'test/**/*.ts' -- prettier --write {{changed}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aloefflerj/astro-focus-kit-server.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api",
    "procrastination",
    "productivity",
    "todo"
  ],
  "author": "Anderson Job Loeffler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aloefflerj/astro-focus-kit-server/issues"
  },
  "homepage": "https://github.com/aloefflerj/astro-focus-kit-server#readme",
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.0",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "body-parser": "^1.20.1",
    "config": "^3.3.8",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "mongoose": "^6.6.5"
  }
}
